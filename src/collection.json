{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "extends": "@schematics/angular",
  "schematics": {
    "atomic-component": {
      "aliases": ["ac"],
      "description": "Create an atomic component",
      "factory": "./atomic-component/index",
      "schema": "./atomic-component/schema.json"
    },
    "atom": {
      "description": "Create an atom.",
      "factory": "./atomic-component/index#atom",
      "schema": "./atomic-component/schema.json"
    },
    "molecule": {
      "description": "Create a molecule.",
      "factory": "./atomic-component/index#molecule",
      "schema": "./atomic-component/schema.json"
    },
    "organism": {
      "description": "Create a organism.",
      "factory": "./atomic-component/index#organism",
      "schema": "./atomic-component/schema.json"
    },
    "template": {
      "description": "Create a template.",
      "factory": "./atomic-component/index#template",
      "schema": "./atomic-component/schema.json"
    },
    "page": {
      "description": "Create a page",
      "factory": "./atomic-component/index#page",
      "schema": "./atomic-component/schema.json"
    },
    "pages": {
      "description": "Create a pages",
      "factory": "./pages/index",
      "schema": "./pages/schema.json"
    },
    "ng-add": {
      "description": "Add angular-atomic-schematics",
      "factory": "./ng-add/index",
      "schema": "./ng-add/schema.json"
    },
    "setup-project": {
      "description": "Setup angular-atomic-schematics",
      "factory": "./setup-project/index",
      "schema": "./ng-add//schema.json",
      "hidden": true
    }
  }
}
